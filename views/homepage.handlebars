{{#each posts as |post| }}
<div class="post-cards row mb-4">
    <div class="col-md-5">
        <div class="top-part">
            <span>{{post.title}}</span>
            <span>Posted by {{post.user.username}} on {{format_date post.createdAt}}</span>
        </div>
        <div class="bottom-part">

        </div>
    </div>
</div>