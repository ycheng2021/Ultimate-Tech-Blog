{{#each posts as |post| }}
  <div class="card post-cards">
  <div class="card-header">
    <span class="post-title">{{post.title}}</span>
    <span class="right-title post-title">Posted on {{format_date post.createdAt}}</span>
  </div>
  <div class="card-body">
    <p class="card-text">{{post.contents}}</p>
  </div>
</div>
{{/each}}

<div class="form-group">
    <button class="btn btn-primary newPost-btn" type="submit">+ New Post</button>
</div>

<script src="/js/dashboard.js"></script>
